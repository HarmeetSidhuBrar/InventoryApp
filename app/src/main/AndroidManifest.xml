<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sidhupvttechltd.inventoryapp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.sidhupvttechltd.inventoryapp1.MainActivity"
            android:label="Products">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sidhupvttechltd.inventoryapp1.EditorActivity"
            android:label="Add new product"
            android:parentActivityName="com.example.sidhupvttechltd.inventoryapp1.MainActivity">
        </activity>
    </application>

</manifest>